    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Laucnh pa_2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/TP/bin/pa1.out",
            "args": [
                "-z",
                "448",
                "-s",
                "15",
                "-a",
                "rs"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch pa_2_ref",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/PA2",
            "args": [
                "-z",
                "448",
                "-s",
                "15",
                "-a",
                "rs"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}